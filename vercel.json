{
  "version": 2,
  "builds": [
    {
      "src": "my-excel-analytics-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "my-excel-analytics-app/build"
      }
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}